{
    "name": "pdir/geocode-cache-bundle",
    "type": "contao-bundle",
    "description": "GeoCode Cache bundle for Contao 4",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "pdir GmbH",
            "homepage": "https://pdir.de"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "4.4.*",
        "symfony/framework-bundle": "^3.3",
        "opencage/geocode": "^2.1"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Pdir\\GeoCodeCacheBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Pdir\\GeoCodeCacheBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pdir\\GeoCodeCacheBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/pdir/geocode-cache-bundle/issues",
        "source": "https://github.com/pdir/geocode-cache-bundle"
    }
}
